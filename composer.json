{
    "name": "nldev/formbuilder",
    "description": "Form builder",
    "license": "MIT",
    "authors": [{
        "name": "nldev",
        "email": "noualdev@gmail.com",
        "homepage": "https://github.com/NoualAli/"
    }],
    "homepage": "https://github.com/NoualAli/formbuilder",
    "keywords": ["Laravel", "FormBuilder"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "NLDev\\FormBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NLDev\\FormBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NLDev\\FormBuilder\\FormBuilderProvider"
            ],
            "aliases": {
                "FormBuilder": "NLDev\\FormBuilder\\Facades\\FormBuilder"
            }
        }
    }
}